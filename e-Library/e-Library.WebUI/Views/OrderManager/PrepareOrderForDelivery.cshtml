@model IEnumerable<e_Library.Core.Models.Order>

    <h2>Prepare Orders for Delivery</h2>

    <br />  

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeliveryMethod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Area)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Suburb)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.QRCode)
        </th>*@

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.CreatedAt)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OrderStatus)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DeliveryMethod)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Area)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Suburb)
    </td>
    @*<td>
        <img class="img-thumbnail" style="height:50px" src="~/Content/QRCodes/@item.QRCode" />
    </td>*@

    <td>
        <a href="@Url.Action("PrepareDelivery","OrderManager", new { Id=item.Id })" class="btn btn-default">Prepare Order</a>
    </td>
</tr>
}

</table>
